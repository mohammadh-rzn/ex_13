<div class="card <%= product.company %>  <%= product.model %> <%= product.price %>" style="width: 18rem;">
    <img class="card-img-top" src="<%= product.img %>" alt="Card image cap">
    <ul class="list-group list-group-flush">
      <li class="list-group-item"><%= product.company %></li>
      <li class="list-group-item"><%= product.model %></li>
      <li class="list-group-item"><%= product.price %></li>
    </ul>
    <div class="card-body">
        <a href="<%= product.href %>" class="btn btn-primary">More</a>
    </div>
  </div>